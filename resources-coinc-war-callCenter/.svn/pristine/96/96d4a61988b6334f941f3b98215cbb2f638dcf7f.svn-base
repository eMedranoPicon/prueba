/* iconos metas */
.viajeJ {
    height: 86px;
    width: 87px;
    margin: 20px 55px 10px 35px;
}

.viajeJTexto {
    background: url(../images/junior/goals/viajehover-texto.png) center no-repeat;
    width: 48px;
    height: 17px;
    margin-left: 25px;
}

.viajeJ a {
    display: block;
    width: 86px;
    height: 87px;
    color: #fff;
    background: url(../images/junior/goals/viajehover.png) center no-repeat;
    border-radius: 100%;
    border: 7px solid #edeada;
}

.viajeJ a:hover {
    border-radius: 100%;
    border: 7px solid black;
}

.viajeJ a span {
    display: none;
}

