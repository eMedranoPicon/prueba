.movimientosCuentaGrafico{
	background: none repeat scroll 0 0 #70a41c;
	border-radius: 10px;
	height: 200px;
	position: relative;
	width: 670px;
	margin-top: 20px;
	-webkit-box-shadow: 0px 4px 2px #d4d4d4;
    -moz-box-shadow: 0px 4px 2px #d4d4d4;
    box-shadow: 0px 4px 2px #d4d4d4;
	behavior: url(/res/coinc/css/PIE.htc);
}
.estiloTooltip{
	background-color: #498f59;
	border-radius: 0 25px 25px;
	box-shadow: -2px 2px 5px 0 rgba(0, 0, 0, 0.6);
	display: none;
	opacity: 0.85;
	padding: 10px 40px 10px 40px;
	position: absolute;
	color: #fff;
	-webkit-box-shadow: -2px 2px 5px 0px rgba(0,0,0,0.6);
	-moz-box-shadow: -2px 2px 5px 0px rgba(0,0,0,0.6);
	box-shadow: -2px 2px 5px 0px rgba(0,0,0,0.6);
	z-index: 5;
	behavior: url(/res/coinc/css/PIE.htc);
}
.estiloTooltip .fecha {
	font-size: 1.3em;
	font-weight: bold;
}
.estiloTooltip .saldo {
	font-size: 1.8em;
	font-weight: bold;
}
.quedanDias {
    color: #fff;
    font-size: 1.2em;
    position: absolute;
    left: 545px;
    bottom: 27%;
}
.objetivo {
    color: #fff;
