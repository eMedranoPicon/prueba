.fechaIzqParticular{
	position:absolute;
	background: url(../images/botones/flechaIzqParticular.png) no-repeat center;
	background-size: 50px 50px;
	width: 50px;
	height: 50px;
	left: -20px;
    top: 70px;
	cursor: pointer;
}
.fechaIzqParticular:hover{
	background: url(../images/botones/flechaIzqParticularHover.png) no-repeat center;
	background-size: 50px 50px;
}

.fechaDerParticular{
