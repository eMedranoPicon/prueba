/* Toggle Styles */

#wrapper {
    padding-left: 0;
    margin: 0;
    -webkit-transition: all 0.5s ease;
    -moz-transition: all 0.5s ease;
    -o-transition: all 0.5s ease;
    transition: all 0.5s ease;
}

#wrapper.toggled {
    //padding-right: 250px;
}

#sidebar-wrapper {
    z-index: 1000;
    position: fixed;
    width: 50%;
    height: 100%;
    right: -50%;
    overflow-y: auto;
    background: #171415;
    -webkit-transition: all 0.5s ease;
    -moz-transition: all 0.5s ease;
    -o-transition: all 0.5s ease;
    transition: all 0.5s ease;
}

#wrapper.toggled #sidebar-wrapper {
    width: 50%;
    right: 0%;
}

#page-content-wrapper {
    width: 100%;
    position: absolute;
    right: 0%;
    -webkit-transition: all 0.5s ease;
    -moz-transition: all 0.5s ease;
    -o-transition: all 0.5s ease;
    transition: all 0.5s ease;
}

#wrapper.toggled #page-content-wrapper {
    position: absolute;
    right: 50%;
}

/* Sidebar Styles */

.sidebar-nav {
    position: absolute;
    top: 0;
    width: 50%;
    margin: 0;
    padding: 0;
    list-style: none;
    
}


@media(min-width:320px) {
    #sidebar-wrapper {
        width: 75%;
        right: -75%
    }   

    #wrapper.toggled #sidebar-wrapper {
        width: 75%;
        right: 0%;
    }

    #wrapper.toggled #page-content-wrapper {
    position: absolute;
    right: 75%;
}
}

@media(min-width:768px) {

    #sidebar-wrapper {
        width: 60%;
        right: -60%
    }   

    #wrapper.toggled #sidebar-wrapper {
        width: 60%;
        right: 0%;
    }

    #wrapper.toggled #page-content-wrapper {
    position: absolute;
    right: 60%;
}



